{
  "name": "stashapp",
  "version": "0.0.1",
  "main": "index.js",
  "bin": "./dist/index.js",
  "license": "MIT",
  "scripts" : {
    "start": "nodemon",
    "build": "tsc",
    "package": "yarn build && pkg package.json --out-path builds"
