      "src/**/*.graphql"
    ]
  },
  "dependencies": {
    "apollo-server": "2.0.0-rc.5",
    "apollo-server-express": "2.0.0-rc.5",
    "express": "4.16.3",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "graphql-import": "0.6.0",
    "reflect-metadata": "0.1.12",
    "sqlite3": "4.0.1",
    "typeorm": "0.2.7"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/express": "4.16.0",
    "@types/graphql": "0.13.1",
    "@types/node": "10.5.0",
    "nodemon": "1.17.5",
    "pkg": "4.3.1",
    "ts-node": "7.0.0",
    "tsconfig-paths": "3.4.1",
    "typescript": "2.9.2"
  }
}
