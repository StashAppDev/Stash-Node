{
  "name": "stashapp",
  "version": "0.0.1",
  "main": "index.js",
  "bin": "./dist/index.js",
  "license": "MIT",
  "scripts" : {
    "start": "nodemon",
    "build": "tsc",
    "package": "yarn build && pkg package.json --out-path builds && sh copy_native_modules.sh && yarn package:ffmpeg",
    "package:ffmpeg": "sh download_ffmpeg.sh",
    "schema": "node regen-types.js",
    "lint": "tslint -c ./tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint --fix -c ./tslint.json 'src/**/*.ts'"
  },
  "pkg": {
    "scripts": [
      "dist/**/*.js"
    ],
    "assets": [
      "src/**/*.graphql"
    ]
  },
  "dependencies": {
    "apollo-server": "2.3.1",
    "apollo-server-express": "2.3.1",
    "commander": "2.19.0",
    "express": "4.16.4",
    "fs-extra": "7.0.1",
    "glob": "7.1.3",
    "graphql": "14.0.2",
    "graphql-tag": "2.10.0",
    "graphql-import": "0.7.1",
    "inquirer": "6.2.1",
    "mathjs": "5.4.0",
    "p-queue": "3.0.0",
    "reflect-metadata": "0.1.12",
    "sqlite3": "4.0.4",
    "tempy": "0.2.1",
    "typeorm": "0.2.9",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/express": "4.16.0",
    "@types/fs-extra": "5.0.4",
    "@types/glob": "7.1.1",
    "@types/graphql": "14.0.3",
    "@types/inquirer": "0.0.43",
    "@types/mathjs": "4.4.4",
    "@types/node": "10.12.18",
    "@types/p-queue": "3.0.0",
    "@types/tempy": "0.2.0",
    "apollo-codegen": "0.20.2",
    "graphql-code-generator": "0.15.2",
    "graphql-codegen-typescript-template": "0.15.2",
    "nodemon": "1.18.9",
    "pkg": "4.3.5",
    "ts-node": "7.0.1",
    "tsconfig-paths": "3.7.0",
    "tslint": "5.12.0",
    "typescript": "3.2.2"
  }
}
