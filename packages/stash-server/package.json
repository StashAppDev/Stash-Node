{
  "name": "stash-server",
  "version": "0.0.1",
  "main": "index.js",
  "bin": "./dist/index.js",
  "license": "MIT",
  "scripts" : {
    "start": "nodemon",
    "build": "rimraf dist && tsc",
    "package": "yarn build && pkg package.json --out-path builds --options max_old_space_size=4096",
    "lint": "tslint -c ./tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint --fix -c ./tslint.json 'src/**/*.ts'",
    "dev:server": "nodemon"
  },
  "pkg": {
    "scripts": [
      "require.js",
      "dist/**/*.js"
    ],
    "assets": [
      "src/**/*.graphql",
      "dist-ui/**/*"
    ]
  },
  "dependencies": {
    "apollo-server": "2.3.1",
    "apollo-server-express": "2.3.1",
    "commander": "2.19.0",
    "compression": "1.7.3",
    "express": "4.16.4",
    "file-type": "10.7.0",
    "fs-extra": "7.0.1",
    "glob": "7.1.3",
    "graphql": "14.1.1",
    "graphql-tag": "2.10.0",
    "graphql-import": "0.7.1",
    "inquirer": "6.2.1",
    "jimp": "0.6.0",
    "knex": "0.16.3",
    "mathjs": "5.4.0",
    "objection": "1.4.0",
    "p-queue": "3.0.0",
    "read-chunk": "3.0.0",
    "sqlite3": "4.0.4",
    "tempy": "0.2.1",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/compression": "0.0.36",
    "@types/express": "4.16.0",
    "@types/file-type": "10.6.0",
    "@types/fs-extra": "5.0.4",
    "@types/glob": "7.1.1",
    "@types/graphql": "14.0.4",
    "@types/inquirer": "0.0.43",
    "@types/mathjs": "4.4.4",
    "@types/node": "10.12.18",
    "@types/p-queue": "3.0.0",
    "@types/read-chunk": "2.1.0",
    "@types/tempy": "0.2.0",
    "nodemon": "1.18.9",
    "pkg": "4.3.7",
    "rimraf": "2.6.3",
    "ts-node": "7.0.1",
    "tsconfig-paths": "3.7.0",
    "tslint": "5.12.0",
    "typescript": "3.2.2"
  }
}
